{
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile " ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "D:\\Logs\\Inventory\\log-{Date}.txt"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "Inventory"
    }
  },
  "Swagger": {
    "title": "Inventory API",
    "projectXmlDocName": "Inventory",
    "description": "Micro service dedicated for inventory.",
    "documentName": "v1",
    "routeTemplate": "api-docs/{documentName}/swagger.json",
    "relativePath": "help"
  }
  
}